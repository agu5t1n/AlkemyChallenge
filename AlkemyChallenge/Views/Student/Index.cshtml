@model List<AlkemyChallenge.Models.ViewModels.ListStudent>
@{
    ViewBag.Title = "Bienvenido";
}

<h2>@ViewBag.Title</h2>
@{
    <div class="row" style="text-align:right;padding:5px">


    </div>
    <div class="row">
        <div class="col-md-12">
            <table class="table">
                <tr>
                    <th>Nombre</th>
                    <th>Apellido</th>
                    <th>DNI</th>
                    <th>Legajo</th>
                </tr>
                @foreach (var student in Model)
                {
                    <tr>
                        <td>@student.First_Name</td>
                        <td>@student.Last_Name</td>
                        <td>@student.DNI</td>
                        <td>@student.File_Student</td>
                    </tr>
                }
            </table>
        </div>
    </div>

}